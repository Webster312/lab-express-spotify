<section id="container">
  {{#each artists.items}}
    <div id="cover">
      <h2>{{name}}</h2>

      <img id="img" src={{images.0.url}} />
</div>
      <form action="/album/{{id}}" method="get">

        <button id="link-button" type="submit">View Albums</button>
      </form>
    
  {{/each}}
</section>